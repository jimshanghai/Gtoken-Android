            android:layout_height="@dimen/dp_18"
            android:src="@drawable/go_p"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp_23"
            />

    </RelativeLayout>
    <View
